FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (35.7632 0 0);

boundaryField
{
    slip
    {
        type            slip;
    }
    sym
    {
        type            symmetry;
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform 
0
;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           uniform (35.7632 0 0);
    }
    lowerWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    wall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    procBoundary5to1
    {
        type            processor;
        value           uniform (35.7632 0 0);
    }
    procBoundary5to2
    {
        type            processor;
        value           uniform (35.7632 0 0);
    }
    procBoundary5to4
    {
        type            processor;
        value           uniform (35.7632 0 0);
    }
}


